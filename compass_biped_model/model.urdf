    <collision name='collision'>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <geometry>
        <box size=".07 .1 .09" />
      </geometry>
    </collision>
  </link>

  <link name='right_foot'>
    <inertial name='inertial'>
      <mass value="0.2" />
      <inertia ixx="0.000193333" ixy="0" ixz="0" iyy="0.000693333" iyz="0" izz="0.000833333" />
    </inertial>
    <visual name='visual'>
      <geometry>
        <box size=".2 .1 .04" />
      </geometry>
    </visual>
    <collision name='collision'>
      <geometry>
        <box size=".2 .1 .04" />
      </geometry>
    </collision>
  </link>

  <link name='left_leg'>
    <inertial name='inertial'>
      <origin xyz="0 0 -0.43" rpy="0 0 0"/>
      <mass value="1.2" />
      <inertia ixx="0.07496" ixy="0" ixz="0" iyy="0.07445" iyz="0" izz="0.00149" />
