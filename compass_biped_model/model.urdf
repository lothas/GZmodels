    </collision>
  </link>

  <link name="pelvis">
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <collision>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

  <!-- joints -->
  <joint name="inner_ankle" type="revolute">
    <origin xyz="0 0 -0.865" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="inner_legs"/>
    <child link="inner_talus"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="220" lower="-1.65" upper="1.3" velocity="12"/>
    <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-2.5" soft_upper_limit="1.5"/>
  </joint>

  <joint name="inner_foot_ligament" type="fixed">
    <origin xyz="0.045 0 -0.115" rpy="0 0 0"/>
    <parent link="inner_talus"/>
    <child link="inner_foot"/>
  </joint>

  <joint name="right_ankle" type="revolute">
    <origin xyz="0 0 -0.865" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="right_leg"/>
    <child link="right_talus"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="220" lower="-1.65" upper="1.3" velocity="12"/>
    <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-2.5" soft_upper_limit="1.5"/>
  </joint>

  <joint name="right_foot_ligament" type="fixed">
    <origin xyz="0.045 0 -0.115" rpy="0 0 0"/>
    <parent link="right_talus"/>
    <child link="right_foot"/>
  </joint>

  <joint name="right_hip" type="revolute">
    <origin xyz="0 -0.17 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="inner_legs"/>
    <child link="right_leg"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="220" lower="-3.14" upper="3.14" velocity="12"/>
    <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-3.04" soft_upper_limit="3.04"/>
  </joint>

  <joint name="left_ankle" type="revolute">
    <origin xyz="0 0 -0.865" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="left_leg"/>
    <child link="left_talus"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="220" lower="-1.65" upper="1.3" velocity="12"/>
    <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-2.5" soft_upper_limit="1.5"/>
  </joint>

  <joint name="left_foot_ligament" type="fixed">
    <origin xyz="0.045 0 -0.115" rpy="0 0 0"/>
    <parent link="left_talus"/>
    <child link="left_foot"/>
  </joint>

  <joint name="left_hip" type="revolute">
    <origin xyz="0 0.17 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <parent link="inner_legs"/>
    <child link="left_leg"/>
    <dynamics damping="0.1" friction="0.0"/>
    <limit effort="220" lower="-3.14" upper="3.14" velocity="12"/>
    <safety_controller k_position="100" k_velocity="100" soft_lower_limit="-3.04" soft_upper_limit="3.04"/>
  </joint>

  <joint name="pelvis_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="inner_legs"/>
    <child link="pelvis"/>
  </joint>

  <gazebo reference="inner_ankle">
    <provideFeedback>1</provideFeedback>
  </gazebo>

  <gazebo reference="right_ankle">
    <provideFeedback>1</provideFeedback>
  </gazebo>

  <gazebo reference="right_hip">
    <provideFeedback>1</provideFeedback>
  </gazebo>

  <gazebo reference="left_ankle">
    <provideFeedback>1</provideFeedback>
  </gazebo>

  <gazebo reference="left_hip">
    <provideFeedback>1</provideFeedback>
  </gazebo>


<!-- imu -->
  <gazebo reference="pelvis">
    <sensor name="imu_sensor" type="imu">
      <always_on>1</always_on>
      <update_rate>1000.0</update_rate>
    </sensor>
  </gazebo>

  <gazebo>
    <!-- robot model offset -->
    <pose>0 0 1 0 0 0</pose>
  </gazebo>

  <gazebo>
    <plugin filename="libControllerPlugin.so" name="robot_plugin" />
    <!--<plugin filename="libgazebo_ros_joint_trajectory.so" name="joint_trajectory_plugin">
      <topicName>joint_trajectory</topicName>
      <updateRate>1000.0</updateRate>
    </plugin>
    <plugin filename="libgazebo_ros_controller_manager.so" name="gazebo_ros_controller_manager">
      <alwaysOn>true</alwaysOn>
      <updateRate>1000.0</updateRate>
    </plugin>-->
  </gazebo>

  <!-- foot contact parameters -->
  <gazebo reference="right_foot">
    <mu1>10.0</mu1>
    <mu2>10.0</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>0.0</maxVel>
    <minDepth>0.001</minDepth>
  </gazebo>
  <gazebo reference="left_foot">
    <mu1>10.0</mu1>
    <mu2>10.0</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>0.0</maxVel>
    <minDepth>0.001</minDepth>
  </gazebo>
  <gazebo reference="inner_foot">
    <mu1>10.0</mu1>
    <mu2>10.0</mu2>
    <fdir1>1 0 0</fdir1>
    <maxVel>0.0</maxVel>
    <minDepth>0.001</minDepth>
  </gazebo>

  <!-- odometry plugin -->
  <gazebo>
    <plugin filename="libgazebo_ros_p3d.so" name="gazebo_ros_p3d">
      <bodyName>pelvis</bodyName>
      <topicName>/ground_truth_odom</topicName>
      <alwaysOn>true</alwaysOn>
      <updateRate>100.0</updateRate>
    </plugin>
  </gazebo>

  <!-- transmissions -->
  <transmission name="right_hip_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="right_hip_motor"/>
    <joint name="right_hip"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="left_hip_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="left_hip_motor"/>
    <joint name="left_hip"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="right_ankle_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="right_ankle_motor"/>
    <joint name="right_ankle"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="left_ankle_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="left_ankle_motor"/>
    <joint name="left_ankle"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="inner_ankle_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="inner_ankle_motor"/>
    <joint name="inner_ankle"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>

  <!-- <gazebo>
  <plugin name="reset_plugin" filename="/usr/local/lib/gazebo_plugins/libGzResetPlugin.so"/>
  </gazebo> -->

</robot>

