      <origin xyz="0 0 -0.43" rpy="0 0 0"/>
      <geometry>
        <box size=".07 .1 .86" />
      </geometry>
    </collision>
  </link>

  <link name='left_talus'>
    <inertial name='inertial'>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <mass value="0.1" />
      <inertia ixx="0.000150833" ixy="0" ixz="0" iyy="0.000108333" iyz="0" izz="0.000124166" />
    </inertial>
    <visual name='visual'>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <geometry>
        <box size=".07 .1 .09" />
